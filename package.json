{
    "name": "reanime-web-frontend",
    "version": "1.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "clean-install": "rimraf ./package-lock.json ./node_modules && npm install",
        "dev:webpack": "rimraf ./.next && dotenv -e .env.development -- next dev --webpack --port 3641 --hostname 192.168.0.105",
        "dev": "rimraf ./.next && dotenv -e .env.development -- next dev --port 3641 --hostname 192.168.0.105",
        "dev-s": "rimraf ./.next && dotenv -e .env.development -- next dev --experimental-https --port 3641 --hostname 192.168.0.105",
        "build:prod": "rimraf ./.next && node --env-file=.env.production ./src/scripts/check-env.ts && dotenv -e .env.production -- next build",
        "build:test": "rimraf ./.next && node --env-file=.env.test ./src/scripts/check-env.ts && dotenv -e .env.test -- next build",
        "start:test": "dotenv -e .env.test -- next start --port 2641 --hostname 192.168.0.105",
        "start:dev": "dotenv -e .env.development -- next start --port 3641 --hostname 192.168.0.105",
        "inspector": "tsc --watch --noEmit --project ./tsconfig.json",
        "inspect": "tsc --noEmit --project ./tsconfig.json",
        "start:production": "dotenv -e .env.production -- next start --port 4641 --hostname 192.168.0.105"
    },
    "dependencies": {
        "@hookform/resolvers": "^5.2.2",
        "@next/third-parties": "^16.0.8",
        "@radix-ui/react-alert-dialog": "^1.1.15",
        "@radix-ui/react-dropdown-menu": "^2.1.16",
        "@radix-ui/react-radio-group": "^1.3.8",
        "@radix-ui/react-select": "^2.2.6",
        "@radix-ui/react-slot": "^1.2.4",
        "better-psl": "^1.0.1",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "consola": "^3.4.2",
        "cookies-next": "^6.1.1",
        "date-fns": "^4.1.0",
        "dotenv-cli": "^11.0.0",
        "embla-carousel-autoplay": "^8.6.0",
        "embla-carousel-react": "^8.6.0",
        "jotai": "^2.16.0",
        "kodik": "^0.0.2",
        "log-it-colored": "^0.0.3",
        "lucide-react": "^0.560.0",
        "maxmind": "^5.0.1",
        "mime-types": "^3.0.2",
        "motion": "^12.23.26",
        "next": "^16.0.8",
        "next-themes": "^0.4.6",
        "react": "^19.2.1",
        "react-dom": "^19.2.1",
        "react-hook-form": "^7.68.0",
        "react-icons": "^5.5.0",
        "react-use": "^17.6.0",
        "safest-env": "^0.0.1",
        "sharp": "^0.34.5",
        "shikimoript": "^0.1.2",
        "tailwind-merge": "^3.4.0",
        "ua-parser-js": "^2.0.7",
        "zod": "^4.1.13"
    },
    "devDependencies": {
        "@prisma/client": "^7.1.0",
        "@tailwindcss/postcss": "^4.1.17",
        "@types/mime-types": "^3.0.1",
        "@types/ms": "^2.1.0",
        "@types/node": "^25.0.0",
        "@types/react": "^19.2.7",
        "@types/react-dom": "^19.2.3",
        "rimraf": "^6.1.2",
        "tailwindcss": "^4.1.17",
        "tw-animate-css": "^1.4.0",
        "typescript": "^5.9.3"
    },
    "engines": {
        "node": ">=24.11.1"
    },
    "imports": {
        "#user-service/*": "./src/shared/generated-shared-export/*"
    }
}
