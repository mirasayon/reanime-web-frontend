{
    "name": "reanime-web-frontend",
    "version": "0.9.3",
    "private": true,
    "type": "module",
    "scripts": {
        "det": "dotenv -e .env.development -- next dev --turbopack --port 3839",
        "dev": "dotenv -e .env.development -- next dev --port 3839",
        "build": "dotenv -e .env.production -- next build",
        "build-prod": "dotenv -e .env.production -- next build --no-lint",
        "cl": "rimraf ./.next ./package-lock.json",
        "start-test": "dotenv -e .env.test -- next start --port 2839",
        "standalone": "dotenv -e .env.production -- node ./.next/standalone/server.js",
        "tsi": "tsc --watch --noEmit --project ./tsconfig.json",
        "start-prod": "dotenv -e .env.production -- next start -p 4839 --hostname 192.168.0.105"
    },
    "dependencies": {
        "@hookform/resolvers": "^5.1.1",
        "@next/third-parties": "^15.4.3",
        "@xamarin.city/reanime": "http://192.168.0.105:8081/xamarin.city-reanime-0.1.0.tgz",
        "axios": "^1.11.0",
        "cookies-next": "^6.1.0",
        "dotenv-cli": "^9.0.0",
        "next": "^15.4.3",
        "next-themes": "^0.4.6",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "react-hook-form": "^7.60.0",
        "react-icons": "^5.5.0",
        "zod": "^4.0.5"
    },
    "devDependencies": {
        "@tailwindcss/postcss": "^4.1.11",
        "@types/node": "^24.1.0",
        "@types/react": "^19.1.8",
        "@types/react-dom": "^19.1.6",
        "autoprefixer": "^10.4.21",
        "postcss": "^8.5.6",
        "postcss-load-config": "^6.0.1",
        "rimraf": "^6.0.1",
        "tailwindcss": "^4.1.11",
        "typescript": "^5.8.3"
    }
}
